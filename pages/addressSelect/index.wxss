/* pages/addressSelect/index.wxss */
page {
  background-color: #efeff4;
  font-size: 10pt;
  -webkit-user-select: none;
  user-select: none;
  width: 100%;
  overflow-x: hidden;
}

.address {
  display: flex;
  flex-wrap: wrap;
  background-color: #fff;
  margin-top: 1px;
  padding: 30rpx;
  margin: 30rpx 0;
}

.address-detail {
  width: 100%;
}

.address-name-phone {
  margin-bottom: 20rpx;
  font-size: 11pt;
}

.address-name{
  margin-right: 20rpx;
}

.address-info {
  margin-bottom: 30rpx;
}

